<graphml xmlns="http://graphml.graphdrawing.org/xmlns"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">
    <desc>TestGraphML_Encode</desc>
    <key id="d0" for="all" attr.name="double" attr.type="double">
        <desc>common double data-function</desc>
        <default>10.2</default>
    </key>
    <key id="d1" for="graph" attr.name="string" attr.type="string"/>
    <key id="d2" for="graph" attr.name="bool" attr.type="boolean"/>
    <key id="d3" for="graph" attr.name="integer" attr.type="int"/>
    <key id="d4" for="node" attr.name="bool" attr.type="boolean"/>
    <key id="d5" for="node" attr.name="integer" attr.type="int"/>
    <key id="d6" for="node" attr.name="string" attr.type="string"/>
    <key id="d7" for="edge" attr.name="bool" attr.type="boolean"/>
    <key id="d8" for="edge" attr.name="integer" attr.type="int"/>
    <key id="d9" for="edge" attr.name="string" attr.type="string"/>
    <graph id="g0" edgedefault="directed">
        <desc>test graph</desc>
        <node id="n0">
            <desc>test node #1</desc>
            <data key="d0"/>
            <data key="d4">false</data>
            <data key="d5">120</data>
            <data key="d6">string data</data>
        </node>
        <node id="n1">
            <desc>test node #2</desc>
            <data key="d6">string data</data>
            <data key="d4">false</data>
            <data key="d5">120</data>
        </node>
        <edge id="e0" source="n0" target="n1">
            <desc>test edge</desc>
            <data key="d7">false</data>
            <data key="d8">120</data>
            <data key="d9">string data</data>
        </edge>
        <data key="d1">string data</data>
        <data key="d0">3.14</data>
        <data key="d2">false</data>
        <data key="d3">120</data>
    </graph>
</graphml>